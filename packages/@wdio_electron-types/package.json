{
  "name": "@wdio/electron-types",
  "version": "7.0.0-beta.17",
  "description": "Types for WebdriverIO Electron Service",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "typeScriptVersion": "5.2.2",
  "scripts": {
    "clean": "pnpm clean:dist && pnpm dlx shx rm -rf ./node_modules pnpm-lock.yaml",
    "clean:dist": "pnpm dlx shx rm -rf ./dist",
    "build": "tsc",
    "pack": "pnpm pack",
    "release": "release-it --ci --npm.skipChecks --no-git.requireCleanWorkingDir"
  },
  "devDependencies": {
    "@electron-forge/shared-types": "^7.4.0",
    "@electron/packager": "^18.3.4",
    "@vitest/spy": "^2.0.5",
    "@wdio/globals": "^9.0.6",
    "@wdio/types": "^9.0.4",
    "builder-util": "25.0.0-alpha.10",
    "electron": "^32.0.1",
    "read-package-up": "^11.0.0",
    "release-it": "^17.6.0",
    "typescript": "^5.5.4",
    "webdriverio": "^9.0.6"
  },
  "files": [
    "dist/*"
  ],
  "release-it": {
    "git": false,
    "npm": {
      "publish": true,
      "publishPath": "*.tgz"
    }
  }
}
