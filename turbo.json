{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "//#format:check": {},
    "//#init-yarn-package-for-forge": {},
    "//#lint": {},
    "@repo/types#build": {},
    "@repo/utils#build": {
      "dependsOn": ["@repo/types#build"]
    },
    "build": {
      "dependsOn": ["^build", "init"]
    },
    "build:mac-universal": {},
    "clean": {
      "cache": false
    },
    "clean:dist": {
      "cache": false
    },
    "clean:package": {
      "cache": false
    },
    "example-forge-esm#init": {
      "dependsOn": ["example-forge-cjs#test:e2e"]
    },
    "example-forge-cjs#build": {
      "dependsOn": ["^build", "example-forge-cjs#init"]
    },
    "example-forge-esm#build": {
      "dependsOn": ["^build", "example-forge-esm#init"]
    },
    "example-builder-cjs#build": {
      "dependsOn": ["^build", "example-forge-esm#test:e2e"]
    },
    "example-builder-esm#build": {
      "dependsOn": ["^build", "example-builder-cjs#test:e2e"]
    },
    "exec": {
      "passThroughEnv": ["DEBUG", "EXAMPLE_DIR"]
    },
    "example-forge-cjs#build:mac-universal": {
      "dependsOn": ["example-forge-esm#build:mac-universal"]
    },
    "example-builder-cjs#build:mac-universal": {
      "dependsOn": ["example-builder-esm#build:mac-universal"]
    },
    "example-builder-cjs#test:e2e": {
      "dependsOn": ["example-builder-cjs#build", "wdio-electron-service#build", "example-forge-esm#test:e2e"]
    },
    "example-builder-esm#test:e2e": {
      "dependsOn": ["example-builder-esm#build", "wdio-electron-service#build", "example-builder-cjs#test:e2e"]
    },
    "example-forge-cjs#test:e2e": {
      "dependsOn": ["example-forge-cjs#build", "wdio-electron-service#build"]
    },
    "example-forge-esm#test:e2e": {
      "dependsOn": ["example-forge-esm#build", "wdio-electron-service#build", "example-forge-cjs#test:e2e"]
    },
    "init": {
      "dependsOn": ["^build", "//#init-yarn-package-for-forge", "wdio-electron-service#test:unit"]
    },
    "init-yarn-package-for-forge": {
      "dependsOn": ["build"]
    },
    "release": {
      "dependsOn": ["build"],
      "cache": false
    },
    "test:e2e": {
      "dependsOn": ["build"]
    },
    "test:unit": {
      "dependsOn": ["^build"]
    },
    "test": {},
    "test:dev": {
      "dependsOn": ["^build"],
      "persistent": true,
      "cache": false
    },
    "wdio-electron-service#build": {
      "dependsOn": ["@repo/utils#build"]
    }
  }
}
